keymap:
  normal:
    h: cursor_move_left
    j: cursor_move_down
    k: cursor_move_up
    l: cursor_move_right
    n:
      cursor_move_down
      cursor_move_right
    default: nop
  insert:
    default: insert_character
    ctrl-alt-c: quit
    ctrl-s: save
    shift-d: delete_word
    arrow_up: cursor_move_up
    super-arrow_up: cursor_move_up
